<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.justin.jetpacklearn">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.MANAGE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JetpackLearn">
        <activity
            android:name=".socket.SocketActivity"
            android:exported="false" />
        <activity
            android:name=".ui.UIActivity"
            android:exported="false" />
        <activity
            android:name=".message.MessageActivity"
            android:exported="false" />
        <activity
            android:name=".io.IOActivity"
            android:exported="false" />
        <activity
            android:name=".mvp.v.MvpActivity"
            android:exported="false" />
        <activity
            android:name=".mvi.v.MVIActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Room.RoomActivity"
            android:exported="false" />
        <activity
            android:name=".Flow.FlowActivity"
            android:exported="false" />
        <activity
            android:name=".ViewModel.ViewModelActivity"
            android:exported="false" />
        <activity
            android:name=".LiveData.LiveDataActivity"
            android:exported="false" />
        <activity
            android:name=".hilt.HiltActivity"
            android:exported="false" />
    </application>

</manifest>